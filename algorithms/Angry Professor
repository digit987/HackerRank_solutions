#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main(){
    int t,n,k,a0,a_i,time,count; 
    scanf("%d",&t);
    for(a0 = 0; a0 < t; a0++){
        scanf("%d %d",&n,&k);
        count=0;
        for(a_i = 0; a_i < n; a_i++){
           scanf("%d",&time);
            if(time<=0)
                {
                count++;
            }
        }
        if(count>=k)
            {
            printf("NO");
        }
        else
            {
            printf("YES");
        }
        printf("\n");
    }
    
    return 0;
}
