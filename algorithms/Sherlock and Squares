#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int t,i,j,k,count=0;
    scanf("%d",&t);
    int A,B;
    for(i=1;i<=t;i++)
        {
        scanf("%d %d",&A,&B);
        printf("%d\n",(int)((floor(sqrt(B)))-(ceil(sqrt(A)))+1));
    
    }
    
    return 0;
}
